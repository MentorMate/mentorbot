{
  "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
  "version": "0.2",
  "language": "en-GB",
  "overrides": [
    {
      "filename": "{*.cmd,*.cs,*.csproj}",
      "language": "en-US"
    },
    {
      "filename": "{.cspell.json,*.svg}",
      "enabled": false
    },
    {
      "filename": "*.md",
      "words": [
        "dpkg",
        "functionapp",
        "westeurope",
        "appsettings",
        "HADOLINT"
      ]
    },
    {
      "filename": "Dockerfile",
      "words": [
        "WORKDIR",
        "wwwroot",
        "aspnet"
      ]
    }
  ],
  "ignorePaths": [
    ".gitattributes",
    ".gitignore",
    ".editorconfig",
    "cspell.json",
    "super-linter.log",
    "**/.gitignore",
    "**/.vscode/**",
    "*.{png,jpg,pdf}",
    "**/*.ruleset",
    "**/coverage/**",
    "**/dist/**",
    "**/bin/**",
    "**/obj/**",
    "**/node_modules/**",
    "**/TestResults/**",
    "**/package*.json",
    "**/tsconfig.json",
    "**/karma.conf.js",
    "**/tslint.json",
    "**/launchSettings.json",
    "**/local.settings.json"
  ],
  "words": [
    "cref",
    "msbuild",
    "anymore",
    "analyzer",
    "analizers",
    "autoprefixer",
    "ruleset",
    "netcoreapp",
    "netstandard",
    "contentfiles",
    "contoso",
    "stylecop",
    "sonarlint",
    "nuget",
    "nupkg",
    "refs",
    "sublicense",
    "menees",
    "autobuild",
    "CodeQL",
    "markdownlint",
    "codecov",
    "Willezone",
    
    "mentorbot",
    "mentorbotlearningcenter",
    "oidc",
    "unsubmitted",
    "timesheet",
    "timesheets",
    "signin",
    "signout",
    "counterclaim",
    "errorlevel",
    "LINQ",
    "NOLOGO",
    "noeq",
    "polyfills",
    "jwks",
    "ngneat",
    "lcov",
    "registerables"
  ]
}