{
  "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
  "version": "0.2",
  "language": "en-GB",
  "overrides": [
    {
      "filename": "{*.cmd,*.cs,*.csproj}",
      "language": "en-US"
    },
    {
      "filename": "{.cspell.json,*.svg}",
      "enabled": false
    }
  ],
  "ignorePaths": [
    ".gitattributes",
    ".gitignore",
    ".editorconfig",
    "cspell.json",
    "**/.gitignore",
    "**/.vscode/**",
    "*.{png,jpg,pdf}",
    "**/*.ruleset",
    "**/coverage/**",
    "**/dist/**",
    "**/node_modules/**",
    "**/package*.json",
    "**/tsconfig.json",
    "**/karma.conf.js"
],
  "words": [
    "msbuild",
    "analyzer",
    "analizers",
    "ruleset",
    "netcoreapp",
    "netstandard",
    "contentfiles",
    "contoso",
    "stylecop",
    "sonarlint",
    "nuget",
    "nupkg",
    "refs",
    "sublicense",
    "menees",
    "autobuild",
    "CodeQL",
    "markdownlint",
    
    "mentorbot",
    "mentorbotlearningcenter",
    "oidc",
    "unsubmitted",
    "timesheet",
    "signin",
    "signout",
    "counterclaim",
    "errorlevel",
    "LINQ",
    "NOLOGO",
    "noeq",
    "polyfills",
    "jwks"
  ]
}